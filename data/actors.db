{"_id":"xTPIJgFeYfur74zo","name":"Pinkwater Security Officer","permission":{"default":0,"gQTQiXvKh15nx34B":3},"type":"npc","data":{"abilities":{"accuracy":{"rating":1,"focus":"pistols","useFocus":false},"communication":{"rating":1,"focus":"","useFocus":false},"constitution":{"rating":1,"focus":"","useFocus":false},"dexterity":{"rating":1,"focus":"","useFocus":false},"fighting":{"rating":2,"focus":"light weapons","useFocus":false},"intelligence":{"rating":0,"focus":"security, tactics","useFocus":false},"perception":{"rating":2,"focus":"searching","useFocus":false},"strength":{"rating":2,"focus":"","useFocus":false},"willpower":{"rating":1,"focus":"","useFocus":false}},"attributes":{"speed":{"value":11,"modified":0,"label":"Speed","type":"number"},"fortune":{"max":0,"value":15,"modified":0,"label":"fortune","type":"number"},"defense":{"value":11,"modified":0,"label":"defense","type":"number"},"artou":{"value":5,"modified":0,"label":"ar+tou","type":"number"}},"threat":"Minor","notes":"<p><span class=\"fontstyle0\">Pinkwater Security is a private security company holding the largest public safety contract on Ganymede. They are responsible for keeping the peace and enforcing laws in all areas outside of direct corporate control and policing. Their officers are well-trained, but the distance from their Martian overseers, as well as a corporate board always looking to cut costs, allows for not-insignificant levels of graft and abuse of power.</span></p>","stunts":"double-team, knock prone, vicious blow","talent1":"pistol style (novice)","talent2":"","equipment1":"baton, medium body armour, pistol, taser","equipment2":""},"folder":"BMcNgwLCkiEwbdrP","sort":600001,"flags":{},"img":"worlds/cupbearer/tokens/tough3.png","token":{"flags":{},"name":"Pinkwater Security Officer","displayName":0,"img":"worlds/cupbearer/tokens/tough3.png","tint":"","width":1,"height":1,"scale":1,"mirrorX":false,"mirrorY":false,"lockRotation":false,"rotation":0,"vision":false,"dimSight":0,"brightSight